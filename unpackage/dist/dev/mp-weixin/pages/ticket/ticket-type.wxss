.view-ticket-type .image-swiper {
  height: 200px;
}
.view-ticket-type .image-swiper .view-image .image-view {
  height: 100%;
  width: 100%;
}
.view-ticket-type .image-swiper .view-image .image-view .ice-image {
  width: 100%;
  height: 100%;
}
.view-ticket-type .image-swiper .view-image .image-text {
  width: 100%;
  position: absolute;
  top: 150px;
}
.view-ticket-type .image-swiper .view-image .image-text .text-view {
  text-align: center;
  padding: 0px 4px 1px 11px;
  background: #adadad;
  color: #ffffff;
  border-radius: 25px;
  margin: 0 auto;
  opacity: 0.9;
}
.view-ticket-type .view-ticket {
  border-top-left-radius: 15px;
  border-top-right-radius: 15px;
  overflow: hidden;
  margin-top: -15px;
  position: absolute;
  background: #ffffff;
  width: 100%;
  padding: 20px 0px 15px 0px;
}
.view-ticket-type .view-ticket .ticket-title {
  font-size: 23px;
  padding: 0px 15px 0px 15px;
}
.view-ticket-type .view-ticket .ticket-explain {
  padding: 2px 15px 10px 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 12px;
  color: #008ee5;
}
.view-ticket-type .view-ticket .ticket-explain .explain-tap {
  background: #d1eeff;
  padding: 1px 8px 1px 8px;
  margin-right: 5px;
  border-radius: 5px;
}
.view-ticket-type .view-ticket .ticket-explain .explain-introduction {
  display: -webkit-box;
  display: -webkit-flex;
